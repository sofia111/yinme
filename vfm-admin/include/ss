  201      play: function() {
  202:       pl.removeClass(this._data.oLink,this._data.className);
  203        this._data.className = pl.css.sPlaying;
  204:       pl.addClass(this._data.oLink,this._data.className);
  205      },
  206  
  207      stop: function() {
  208:       pl.removeClass(this._data.oLink,this._data.className);
  209        this._data.className = '';
  210      },
  211  
  212      pause: function() {
  213:       pl.removeClass(this._data.oLink,this._data.className);
  214        this._data.className = pl.css.sPaused;
  215:       pl.addClass(this._data.oLink,this._data.className);
  216      },
  217  
  218      resume: function() {
  219:       pl.removeClass(this._data.oLink,this._data.className);
  220        this._data.className = pl.css.sPlaying;
  221:       pl.addClass(this._data.oLink,this._data.className);      
